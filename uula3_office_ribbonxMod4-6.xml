                                                onAction="NormalText.NormalText" />
                                        <button id="Button21" imageMso="MessageHeaderToggle"
                                                size="normal" label="Topic Name"
                                                screentip="Format as Topic Name"
                                                onAction="TopicName.TopicName" />
                                        <button id="Button22" imageMso="Bullets" size="normal"
                                                label="Bullet Text"
                                                screentip="Format as Bulleted Text"
                                                onAction="BulletText.BulletText" />
                                </group>

                                <group id="SimpleControls3" label="English Style Guide">
                                        <button id="Button14" imageMso="TextBoxDrawMenu"
                                                size="normal" label="Answer"
                                                screentip="Format as Answer"
                                                onAction="AnswerFormatEN.AnswerFormatEN" />
                                        <button id="Button15" imageMso="SmartArtInsertBulletRTL"
                                                size="normal" label="Bulleted Answer"
                                                screentip="Format as Bulleted Answer"

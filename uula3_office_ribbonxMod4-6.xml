                                                size="normal" label="Blanks"
                                                screentip="Add blanks to the assigned tab stop"
                                                onAction="BlankLines2.BlankLines2" />
                                        <button id="Button23" imageMso="EquationEdit" size="normal"
                                                label="Equation Answer"
                                                screentip="Format as an Equation within an Answer"
                                                onAction="EquationAns.EquationAns" />
                                        <button id="Button28" imageMso="ParagraphSpaceBefore"
                                                size="normal" label="No Space"
                                                onAction="No_space.set_gap" />
                                        <button id="Button29" imageMso="ParagraphSpaceBefore"
                                                size="normal" label="Single Space"
                                                onAction="Single_space.set_gap" />
                                        <button id="Button30" imageMso="ParagraphSpaceBefore"
                                                size="normal" label="Triple Spaces"
                                                onAction="Triple_spaces.set_gap" />
                                        <button id="Button31" imageMso="ParagraphSpaceBefore"
                                                size="normal" label="Custom Spacing"
                                                onAction="CustomSpace.set_gap" />
                                        <button id="Button32" imageMso="ObjectsGroup" size="normal"
                                                label="Merge Textboxes"
                                                onAction="MergeTextBoxes.MergeTextBoxes" />

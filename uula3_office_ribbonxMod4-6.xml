<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui">
        <ribbon startFromScratch="false">
                <tabs>
                        <tab id="CustomTab" insertBeforeMso="TabInsert" label="UULA">
                                <group id="SimpleControls" label="Getting Started">
                                        <button id="Button1" imageMso="ThemeFontsGallery"
                                                size="large" label="UULA Sans"
                                                screentip="Change font to UULA Sans"
                                                onAction="FontChange.FontChange" />
                                        <button id="Button2" imageMso="ThemeFontsGallery"
                                                size="large" label="Font Size"
                                                screentip="Change font Size"
                                                onAction="ChangeFontSize.ChangeFontSize" />
                                        <button id="Button34" imageMso="ThemeFontsGallery"
                                                size="normal" label="SmartArt Font"
                                                onAction="SmartArtFont.SmartArtFont" />
                                        <button id="Button3" imageMso="TableInsert" size="normal"
                                                label="Table" screentip="Change Table Fonts"
                                                onAction="SetTableFontMod3.SetTableFontMod3" />
                                        <button id="Button33" imageMso="TableInsert" size="normal"
                                                label="Table Full Width"
                                                onAction="TableWidth.TableWidth" />
                                        <button id="Button4" imageMso="SmartArtAddBullet"
                                                size="normal" label="Bullets"
                                                screentip="Change Bullet Character and Indentation"
                                                onAction="Bullets.Bullets" />
                                        <button id="Button5" imageMso="HeaderInsertGallery"
                                                size="normal" label="Unit Header"
                                                screentip="Change the color of unit header"
                                                onAction="UnitColor.UnitColor" />
                                </group>

                                <group id="SimpleControls2" label="Arabic Style Guide">
                                        <button id="Button8" imageMso="TextBoxDrawMenu"
                                                size="normal" label="Answer"
                                                screentip="Format as Answer"
                                                onAction="AnswerFormat.AnswerFormat" />
                                        <button id="Button9" imageMso="SmartArtInsertBulletRTL"
                                                size="normal" label="Bulleted Answer"
                                                screentip="Format as Bulleted Answer"
                                                onAction="BulletAnswers.BulletAnswers" />
                                        <button id="Button10" imageMso="TextBoxDrawMenu"
                                                size="normal" label="Question Header"
                                                screentip="Format as Question Header"
                                                onAction="QuestionHeader.QuestionHeader" />
                                        <button id="Button11" imageMso="SmartArtInsertBulletRTL"
                                                size="normal" label="Question"
                                                screentip="Format as Question"
                                                onAction="Questions.Questions" />
                                        <button id="Button12" imageMso="TextBoxWordClassic"
                                                size="normal" label="Complete Answer"
                                                screentip="Format as Complete Answer"
                                                onAction="CompleteAns.CompleteAns" />
